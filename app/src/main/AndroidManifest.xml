<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kanjo.health.e_diet.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.kanjo.health.e_diet.app.PagerTest"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.kanjo.health.e_diet.app.GroupFoodActivity"
            android:label="Group Activity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kanjo.health.e_diet.app.PagerTest" />
        </activity>
        <activity
            android:name=".AlimentosListActivity"
            android:label="@string/title_activity_alimentos" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kanjo.health.e_diet.app.GroupFoodActivity" />
        </activity>
    </application>

</manifest>
